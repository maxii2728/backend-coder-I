<h1>Carrito de Compras</h1>
<table>
    <thead>
        <tr>
            <th>Producto</th>
            <th>Cantidad</th>
        </tr>
    </thead>
    <tbody>
        {{#each cart.products}}
            <tr>
                <td>{{this.product}}</td>
                <td>{{this.quantity}}</td>
                
            </tr>
        {{/each}}
    </tbody>
</table>